<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CannyViewAnimator">
        <attr name="type" format="enum">
            <enum name="sequentially" value="1"/>
            <enum name="together" value="2"/>
        </attr>
        <attr name="property_in" format="enum">
            <enum name="empty" value="0"/>
            <enum name="alpha" value="1"/>
            <enum name="alpha_half" value="2"/>
            <enum name="scale_x" value="3"/>
            <enum name="scale_x_half" value="4"/>
            <enum name="scale_y" value="5"/>
            <enum name="scale_y_half" value="6"/>
        </attr>
        <attr name="property_out" format="enum">
            <enum name="empty" value="0"/>
            <enum name="alpha" value="1"/>
            <enum name="alpha_half" value="2"/>
            <enum name="scale_x" value="3"/>
            <enum name="scale_x_half" value="4"/>
            <enum name="scale_y" value="5"/>
            <enum name="scale_y_half" value="6"/>
        </attr>
        <attr name="reveal_in" format="enum">
            <enum name="top_center" value="0"/>
            <enum name="top_left" value="1"/>
            <enum name="top_right" value="2"/>
            <enum name="bottom_center" value="3"/>
            <enum name="bottom_left" value="4"/>
            <enum name="bottom_right" value="5"/>
            <enum name="center_left" value="6"/>
            <enum name="center_right" value="7"/>
            <enum name="center" value="8"/>
        </attr>
        <attr name="reveal_out" format="enum">
            <enum name="top_center" value="0"/>
            <enum name="top_left" value="1"/>
            <enum name="top_right" value="2"/>
            <enum name="bottom_center" value="3"/>
            <enum name="bottom_left" value="4"/>
            <enum name="bottom_right" value="5"/>
            <enum name="center_left" value="6"/>
            <enum name="center_right" value="7"/>
            <enum name="center" value="8"/>
        </attr>
    </declare-styleable>
</resources>